	Keywords = {auroral ionosphere; downward current region; ionospheric modeling; plasma loss; 2407 Ionosphere: Auroral ionosphere (2704); 2431 Ionosphere: Ionosphere/magnetosphere interactions (2736); 2435 Ionosphere: Ionospheric disturbances; 2437 Ionosphere: Ionospheric dynamics; 2447 Ionosphere: Modeling and forecasting},
	Month = {06},
	Number = {A6},
	Pages = {A06306},
	Publisher = {AGU},
	Title = {Ionospheric plasma transport and loss in auroral downward current regions},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1029/2012JA017637},
	Volume = {117},
	Year = {2012},
